    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-fff": "~0.4.2",
    "remark-footnotes": "~2.0.0",
    "shiki-twoslash": "^3.1.1",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.3",
    "sveltekit-embed": "^0.0.13",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unist-util-visit": "^4.1.2",
    "unocss": "^0.50.6",
    "vite": "^4.2.0",
